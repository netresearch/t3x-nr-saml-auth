# Git attributes for nr_saml_auth extension

# Auto detect text files and perform LF normalization
* text=auto

# PHP files
*.php text eol=lf

# Configuration files
*.yaml text eol=lf
*.yml text eol=lf
*.json text eol=lf
*.xml text eol=lf
*.neon text eol=lf

# Documentation
*.rst text eol=lf
*.md text eol=lf

# Scripts
*.sh text eol=lf

# Exclude from release archives (TER/Composer)
/.Build export-ignore
/.ddev export-ignore
/.github export-ignore
/Build export-ignore
/Tests export-ignore
/Documentation export-ignore
/.editorconfig export-ignore
/.gitattributes export-ignore
/.gitignore export-ignore
/.php-cs-fixer.php export-ignore
/phpstan.neon export-ignore
/phpstan-baseline.neon export-ignore
/renovate.json export-ignore
/AGENTS.md export-ignore
